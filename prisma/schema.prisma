generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tb_backend {
  ID   Int    @id
  Name String

  @@map("tb-backend")
}

model menu {
  no    Int    @id @default(autoincrement())
  name  String @db.VarChar(255)
  price String @db.VarChar(255)
}
